{
    "timestep": 0.01,
    "urdf": "pycito/systems/A1/A1_description/urdf/a1_foot_collision.urdf",
    "environment": "FlatGroundEnvironment",
    "controller": {
        "type": "A1ContactEILController",
        "timestep": 0.01,
        "horizon": 5,
        "reference_path": "data/a1/reference/symmetric/3m/reftraj.pkl",
        "lcptype": "ConstantRelaxedPseudoLinearComplementarityConstraint",
        "cost": {
            "base_position": 1e2,
            "joint_position": 1e2,
            "velocity": 1e-2,
            "control": 1e-3,
            "force": 0,
            "slack": 0,
            "jlimit": 0,
            "complementarity_schedule": [1e-2, 1e-4]
        },
        "solver_config": {
            "major_feasibility_tolerance": 1e-5,
            "major_optimality_tolerance": 1e-5,
            "scale_option": 0,
            "major_step_limit": 2.0,
            "superbasics_limit": 1000,
            "linesearch_tolerance": 0.9,
            "iterations_limit": 10000,
            "use_basis_file": true
        },
        "estimator": {
            "horizon": 1,
            "cost": {
                "force": 1e0,
                "distance": 1,
                "friction": 1,
                "velocity_scale": 1e-3,
                "force_scale": 1e-3,
                "relaxation": 1e3
            },
            "contact_model":{
                "type": "SemiparametricContactModel",
                "surface": {
                    "type": "SemiparametricModel",
                    "prior": {
                        "type": "FlatModel",
                        "location": 0,
                        "direction": [0, 0, 1]
                    },
                    "kernel": {
                        "type": "LinearKernel",
                        "weights": [
                            [1, 0, 0],
                            [0, 1, 0],
                            [0, 0, 1]
                        ],
                        "offset": 0
                    }
                },
                "friction": {
                    "type": "SemiparametricModel",
                    "prior": {
                        "type": "ConstantModel",
                        "const": 1.0
                    },
                    "kernel": {
                        "type": "ConstantKernel",
                        "const": 1.0
                    }
                }
            },
            "lcp": "ConstantRelaxedPseudoLinearComplementarityConstraint",
            "solver": {
                "major_feasibility_tolerance": 1e-6,
                "major_optimality_tolerance": 1e-6
            }
        }
    },
    "type": "A1DrakeSimulationBuilder"
}